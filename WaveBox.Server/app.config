<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<!-- Sqlite3 -->
	<dllmap dll="sqlite3" target="lib_native/libsqlite3.0.dylib" os="osx" />
	<dllmap dll="sqlite3" target="lib_native/sqlite3.dll" os="windows" />
	<dllmap dll="sqlite3" target="lib_native/libsqlite3.x86.so" os="linux" wordsize="32" />
	<dllmap dll="sqlite3" target="lib_native/libsqlite3.x64.so" os="linux" wordsize="64" />
	
	<!-- ImageMagick -->
	<dllmap dll="libMagickWand" target="lib_native/libMagickWand-6.Q16-Static.universal.dylib" os="osx" />
	<dllmap dll="libMagickWand" target="lib_native/libMagickWand.dll" os="windows" />
	<dllmap dll="libMagickWand" target="lib_native/libMagickWand-6.Q16-Static.x86.so" os="linux" wordsize="32" />
	<dllmap dll="libMagickWand" target="lib_native/libMagickWand-6.Q16-Static.x64.so" os="linux" wordsize="64" />
	
	<!-- BASS -->
	<dllmap dll="bass.dll" target="lib_native/libbass.dylib" os="osx" />
	<dllmap dll="bass.dll" target="lib_native/bass.dll" os="windows" />
	<dllmap dll="bass.dll" target="lib_native/libbass.x86.so" os="linux" wordsize="32" />
	<dllmap dll="bass.dll" target="lib_native/libbass.x64.so" os="linux" wordsize="64" />
	
	<!-- FSEvents -->
	<dllmap dll="libWaveBoxFSEvents" target="lib_native/libWaveBoxFSEvents.dylib" os="osx" />
	
</configuration>
